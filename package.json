{
  "name": "github-api",
  "version": "1.0.0",
  "description": "Github-Api example demo app",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "pre-commit": "lint-staged",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "lint-staged": {
    "*.{js,json,html,md}": [
      "prettier --write",
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Manas Dwivedi",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.14.5",
    "axios": "^0.21.1",
    "babel-node": "0.0.1-security",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "mongoose": "^5.12.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1"
  }
}
