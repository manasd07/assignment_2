{
  "name": "github-api",
  "version": "1.0.0",
  "description": "Github-Api example demo app",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "pre-commit": "lint-staged",
    "lint": "eslint . --ext .js"
  },
  "author": "Manas Dwivedi",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.14.5",
    "axios": "^0.21.1",
    "babel-node": "0.0.1-security",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "express": "^4.17.1",
    "mongoose": "^5.12.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1"
  }
}
